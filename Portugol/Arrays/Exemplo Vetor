programa
{
	
	funcao inicio()
	{
		real notas[5], media,  soma = 0.0
		inteiro matriculas[5], i		

		para (i = 0 ; i < 5 ; i++){
			escreva("digite a matricula: ")
			leia(matriculas[i]) 
			escreva("Digite a nota da aluno: ")
			leia(notas[i])
			soma = soma + notas[i]
			
		}
		media = soma/5

		escreva ("Matriculas acima da média \n")
		para (i = 0 ; i < 5 ; i++){
			se( notas[i] >= media){
				escreva(matriculas[i], "\n")
				
			}
		}
	}
}
/* $$$ Portugol Studio $$$ 
 * 
 * Esta seção do arquivo guarda informações do Portugol Studio.
 * Você pode apagá-la se estiver utilizando outro editor.
 * 
 * @POSICAO-CURSOR = 369; 
 * @PONTOS-DE-PARADA = ;
 * @SIMBOLOS-INSPECIONADOS = {notas, 6, 7, 5};
 * @FILTRO-ARVORE-TIPOS-DE-DADO = inteiro, real, logico, cadeia, caracter, vazio;
 * @FILTRO-ARVORE-TIPOS-DE-SIMBOLO = variavel, vetor, matriz, funcao;
 */
